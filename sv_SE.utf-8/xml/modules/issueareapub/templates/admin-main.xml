		<string>&lt;b&gt;IssueAreaEditor&lt;/b&gt; (DeleteIssueAreaPub privilege)</string>
		<translation></translation>
		<references>
			<reference file="modules/issueareapub/xartemplates/admin-main.xd" line="48" />
		</references>
	</entry>
	<entry>
		<string>Can create, edit and delete stories or issues.  Cannot publish issues.</string>
		<translation></translation>
		<references>
			<reference file="modules/issueareapub/xartemplates/admin-main.xd" line="50" />
		</references>
	</entry>
